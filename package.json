    "mp3-to-wav": "^0.1.11",
    "needle": "^2.6.0",
    "node-pg-migrate": "^4.6.1",
    "npm-watch": "^0.9.0",
    "pg": "^7.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
